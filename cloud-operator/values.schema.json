{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "cilium": {
            "type": "object",
            "properties": {
                "namespaces": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "clusterCredsSecret": {
            "type": "object",
            "properties": {
                "clientId": {
                    "type": "string"
                },
                "clientSecret": {
                    "type": "string"
                },
                "disableSecretCreation": {
                    "type": "boolean"
                },
                "forceSecretCreation": {
                    "type": "boolean"
                }
            }
        },
        "defaultNamespace": {
            "type": "string"
        },
        "env": {
            "type": "object",
            "properties": {
                "httpsProxy": {
                    "type": "string"
                },
                "ipfixCollectorPort": {
                    "type": "integer"
                },
                "onboardingEndpoint": {
                    "type": "string"
                },
                "streamKeepalivePeriods": {
                    "type": "object",
                    "properties": {
                        "configuration": {
                            "type": "string"
                        },
                        "kubernetesNetworkFlows": {
                            "type": "string"
                        },
                        "kubernetesResources": {
                            "type": "string"
                        },
                        "logs": {
                            "type": "string"
                        }
                    }
                },
                "streamSuccessPeriods": {
                    "type": "object",
                    "properties": {
                        "auth": {
                            "type": "string"
                        },
                        "connect": {
                            "type": "string"
                        }
                    }
                },
                "tlsSkipVerify": {
                    "type": "boolean"
                },
                "tokenEndpoint": {
                    "type": "string"
                },
                "verboseDebugging": {
                    "type": "boolean"
                }
            }
        },
        "falco": {
            "type": "object",
            "properties": {
                "customRules": {
                    "type": "object",
                    "properties": {
                        "rules-networking.yaml": {
                            "type": "string"
                        }
                    }
                },
                "enabled": {
                    "type": "boolean"
                },
                "falco": {
                    "type": "object",
                    "properties": {
                        "buffered_outputs": {
                            "type": "boolean"
                        },
                        "http_output": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "insecure": {
                                    "type": "boolean"
                                },
                                "keep_alive": {
                                    "type": "boolean"
                                },
                                "url": {
                                    "type": "string"
                                }
                            }
                        },
                        "json_include_output_property": {
                            "type": "boolean"
                        },
                        "json_include_tags_property": {
                            "type": "boolean"
                        },
                        "json_output": {
                            "type": "boolean"
                        }
                    }
                },
                "namespace": {
                    "type": "string"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "nodeSelector": {
            "type": "object"
        },
        "onboardingSecret": {
            "type": "object",
            "properties": {
                "clientId": {
                    "type": "string"
                },
                "clientSecret": {
                    "type": "string"
                }
            }
        },
        "openshift": {
            "type": "object",
            "properties": {
                "ovnkNamespace": {
                    "type": "string"
                },
                "workerNodeCidrs": {
                    "type": "array"
                }
            }
        },
        "priorityClassName": {
            "type": "string"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "clusterIP": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "automount": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
